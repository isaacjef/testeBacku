{
  "compilerOptions": {
    "target": "ES6",
       "experimentalDecorators": true,
    // "emitDecoratorMetadata": true,
    "module": "CommonJS",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "outDir": "./bin",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
	
